

<div id="map" style="width: <%= @map.size_x %>px; height: <%= @map.size_y %>px">
	<% @ores.each do |ore| %>
		<%= render("/ore_nodes/ore", :@ore => ore, :@user_confirmed => ore.user_confirmed?(@user_ip)) %>
	<% end %>

	<%= image_tag "maps/#{@map.image}", id: "map_image" %>
</div>

<script type="text/javascript">
	active_map_id = <%= @map.id %>
	$("[rel=tooltip]").tooltip()
</script>
